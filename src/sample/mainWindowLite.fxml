            <Tab text="Browser Tab">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="12.0" prefWidth="960.0" />
               </content>
            </Tab>
         </tabs>
      </TabPane>
      <TextField fx:id="linkTextField" layoutX="100.0" layoutY="30.0" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="32.0" prefWidth="762.0" promptText="Enter link" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="98.0" AnchorPane.topAnchor="30.0" />
